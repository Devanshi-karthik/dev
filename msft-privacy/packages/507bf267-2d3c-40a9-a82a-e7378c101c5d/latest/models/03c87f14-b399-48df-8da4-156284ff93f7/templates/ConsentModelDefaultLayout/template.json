    {
      "name": "Banner",
      "content": "<style>\r\n    .msft-banner-box {\r\n        position: fixed;\r\n        bottom: 0;\r\n        left: 0;\r\n        z-index: 10;\r\n        width: 100%;\r\n        padding: 1rem 1rem;\r\n        background: white;\r\n        box-shadow: inset 0px 1px 0px rgb(204 204 204 / 80%);\r\n    }\r\n\r\n    .msft-content {\r\n        margin-bottom: 16px;\r\n    }\r\n\r\n    .msft-content-description {\r\n        font-family: \"Segoe UI\";\r\n        font-style: normal;\r\n        font-weight: 400;\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n    }\r\n\r\n    .msft-button-primary {\r\n        box-sizing: border-box;\r\n        display: inline-block;\r\n        width: fit-content;\r\n        padding: 0 16px;\r\n        height: 24px;\r\n        border-radius: 2px;\r\n        background-color: #0078d4;\r\n        color: #fff;\r\n        text-decoration: none;\r\n        text-align: center;\r\n        font-family: \"Segoe UI\";\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n        cursor: pointer;\r\n        border: none;\r\n        margin-right: 10px;\r\n    }\r\n\r\n    .msft-button-secondary {\r\n        color: #0078d4;\r\n        cursor: pointer;\r\n        background-color: transparent;\r\n        font-family: \"Segoe UI\";\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n    }\r\n</style>\r\n\r\n<div class=\"msft-banner-box\">\r\n    <div class=\"msft-content\">\r\n        <div class=\"msft-content-description\">{{{description}}}</div>\r\n    </div>\r\n    <div class=\"msft-model-buttonactions\">\r\n        <button class=\"msft-button-primary\" id=\"msft-consent-accept\">\r\n            {{{acceptButton}}}\r\n        </button>\r\n        <button class=\"msft-button-primary\" id=\"msft-consent-decline\">\r\n            {{{declineButton}}}\r\n        </button>\r\n        <span class=\"msft-button-secondary\" id=\"msft-consent-settings-open\">{{{customizeButton}}}</span>\r\n    </div>\r\n</div>\r\n<script>\r\n    document.getElementById(\"msft-consent-accept\").onclick = function () {\r\n        MSFTConsent.trigger(\"AcceptAll\");\r\n        MSFTConsent.trigger(\"CloseConsentNotice\");\r\n    };\r\n    document.getElementById(\"msft-consent-settings-open\").onclick = function () {\r\n        MSFTConsent.trigger(\"ShowTemplate\", { \"templateId\": \"Preferences\" });\r\n        MSFTConsent.trigger(\"CloseConsentNotice\");\r\n    };\r\n    document.getElementById(\"msft-consent-decline\").onclick = function () {\r\n        MSFTConsent.trigger(\"RejectAll\");\r\n        MSFTConsent.trigger(\"CloseConsentNotice\");\r\n    };\r\n</script>",
      "location": "ModelLayout/banner.hbs",
      "fields": [
        "description",
        "acceptButton",
        "declineButton",
        "customizeButton"
      ]
    },
    {
      "name": "Preferences",
      "content": "<style>\r\n    .msft-content {\r\n        margin-bottom: 16px\r\n    }\r\n\r\n    .msft-content-header {\r\n        margin-bottom: 16px;\r\n        font-family: \"Segoe UI\";\r\n        font-style: normal;\r\n        font-weight: 600;\r\n        font-size: 18px;\r\n        line-height: 24px\r\n    }\r\n\r\n    .msft-content-description {\r\n        font-family: \"Segoe UI\";\r\n        font-style: normal;\r\n        font-weight: 400;\r\n        font-size: 13px;\r\n        line-height: 21px\r\n    }\r\n\r\n    .msft-button-primary {\r\n        box-sizing: border-box;\r\n        display: inline-block;\r\n        width: fit-content;\r\n        padding: 0 16;\r\n        height: 24px;\r\n        border-radius: 2px;\r\n        background-color: #0078d4;\r\n        color: #fff;\r\n        text-decoration: none;\r\n        text-align: center;\r\n        font-family: \"Segoe UI\";\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n        cursor: pointer;\r\n        border: none;\r\n        margin-right: 10px\r\n    }\r\n\r\n    .msft-content-trackerCategory-header {\r\n        font-family: \"Segoe UI\";\r\n        font-style: normal;\r\n        font-weight: 600;\r\n        font-size: 14px;\r\n        line-height: 20px;\r\n        display: flex\r\n    }\r\n\r\n    .msft-cookie-box {\r\n        z-index: 10;\r\n        background: #fff;\r\n        box-shadow: 0px 3px 8px rgba(0,0,0,.14);\r\n        border-radius: 2px;\r\n        padding: 1rem\r\n    }\r\n\r\n    .msft-close-box {\r\n        cursor: pointer;\r\n        position: absolute;\r\n        top: 0;\r\n        right: 0;\r\n        margin-top: 10px;\r\n        margin-right: 10px\r\n    }\r\n\r\n    .msft-categories {\r\n        margin-bottom: 25px\r\n    }\r\n\r\n    .msft-preferenceCentre-box {\r\n        width: 632px;\r\n        height: fit-content;\r\n        max-height: 90%;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%,-50%);\r\n        position: fixed;\r\n        overflow-y: auto\r\n    }\r\n\r\n    .msft-content-trackerCategory-toggle {\r\n        outline: transparent;\r\n        position: relative;\r\n        font-size: 20px;\r\n        box-sizing: border-box;\r\n        width: 40px;\r\n        height: 20px;\r\n        border-radius: 10px;\r\n        transition: all .1s ease 0s;\r\n        border: 1px solid #605e5c;\r\n        background: #fff;\r\n        cursor: pointer;\r\n        display: flex;\r\n        align-items: center;\r\n        padding: 0 3px;\r\n        overflow: visible;\r\n        margin: 0 3px\r\n    }\r\n\r\n    .msft-content-trackerCategory-slider {\r\n        display: block;\r\n        width: 12px;\r\n        height: 12px;\r\n        border-radius: 50%;\r\n        transition: all .1s ease 0s;\r\n        background-color: #605e5c;\r\n        border-color: transparent;\r\n        border-width: 6px;\r\n        border-style: solid;\r\n        box-sizing: border-box\r\n    }\r\n\r\n    input:checked + .msft-content-trackerCategory-label > div {\r\n        background: #0078d4;\r\n        justify-content: flex-end\r\n    }\r\n\r\n        input:checked + .msft-content-trackerCategory-label > div > span {\r\n            background-color: #fff\r\n        }\r\n</style>\r\n<div class=\"msft-cookie-box msft-preferenceCentre-box\">\r\n    <span class=\"msft-close-box\" id=\"msft-consent-settings-close\">&times;</span>\r\n    <div class=\"msft-content\">\r\n        <div class=\"msft-content-header\">{{{preferenceHeader}}}</div>\r\n        <div class=\"msft-content-description\">{{{preferenceDescription}}}</div>\r\n    </div>\r\n    <div>\r\n    {{#each trackerCategory}}\r\n        <div class=\"msft-categories\">\r\n            <div class=\"msft-content-trackerCategory-header\">\r\n                <div>{{{Header}}}</div>\r\n                <input category id={{{name}}} type=\"checkbox\" style=\"display:none\"></input>\r\n                <label for={{{name}}} class=\"msft-content-trackerCategory-label\" style=\"display:none\">\r\n                    <div class=\"msft-content-trackerCategory-toggle\">\r\n                        <span class=\"msft-content-trackerCategory-slider\"></span>\r\n                    </div>\r\n                </label>\r\n            </div>\r\n            <div class=\"msft-content-description\">{{{Description}}}</div>\r\n        </div>\r\n        {{/each}}\r\n    </div>\r\n    <div class=\"msft-model-buttonactions\">\r\n        <button class=\"msft-button-primary\" id=\"msft-consent-settings-save\">{{{saveButton}}}</button>\r\n    </div>\r\n</div>\r\n<script>\r\n    window.addEventListener(\"DOMContentLoaded\", function () {\r\n        var trackerCategoriesSelectors = document.querySelectorAll(\".msft-content-trackerCategory-header\");\r\n        MSFTConsent.getConsent().then(function (consent) {\r\n            MSFTConsent.getTrackerCategories().then(function (categories) {\r\n                var preferences = {};\r\n                categories.forEach(function (category) {\r\n                    return preferences[category.categoryName] = category.isEssential;\r\n                });\r\n                trackerCategoriesSelectors.forEach(function (category) {\r\n                    category.getElementsByTagName(\"input\")[0].checked = 1 == consent.preferences[category.getElementsByTagName(\"input\")[0].id];\r\n                    if (!preferences[category.getElementsByTagName(\"input\")[0].id]) {\r\n                        category.getElementsByTagName(\"label\")[0].style.display = \"inline-block\";\r\n                    }\r\n                });\r\n            });\r\n        });\r\n    });\r\n    document.getElementById(\"msft-consent-settings-save\").onclick = function () {\r\n        var trackerCategoriesSelectors = document.querySelectorAll(\".msft-content-trackerCategory-header\");\r\n        var currentPreferences = { preferences: {} };\r\n        trackerCategoriesSelectors.forEach(function (category) {\r\n            currentPreferences.preferences[category.getElementsByTagName(\"input\")[0].id] = category.getElementsByTagName(\"input\")[0].checked ? \"1\" : \"0\";\r\n        });\r\n        MSFTConsent.trigger(\"UpdateConsent\", { consent: currentPreferences });\r\n        MSFTConsent.trigger(\"CloseTemplate\", { \"templateId\": \"Preferences\" });\r\n    }; document.getElementById(\"msft-consent-settings-close\").onclick = function () {\r\n        MSFTConsent.trigger(\"CloseTemplate\", { \"templateId\": \"Preferences\" });\r\n        MSFTConsent.trigger(\"ShowConsentNotice\");\r\n    };\r\n</script>\r\n",
      "location": "ModelLayout/preferenceCenter.hbs",
      "fields": [
        "preferenceHeader",
        "preferenceDescription",
        "categories",
        "saveButton"
      ]
    },
    {
      "name": "Dialog",
      "content": "<style>\r\n    .msft-dialog-box {\r\n        width: 652px;\r\n        height: fit-content;\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%, -50%);\r\n        position: fixed;\r\n    }\r\n\r\n    .msft-cookie-box {\r\n        z-index: 10;\r\n        background: #fff;\r\n        box-shadow: 0px 3px 8px rgba(0, 0, 0, 0.14);\r\n        border-radius: 2px;\r\n        padding: 1rem 1rem;\r\n    }\r\n\r\n    .msft-content-header {\r\n        margin-bottom: 16px;\r\n        font-family: \"Segoe UI\";\r\n        font-style: normal;\r\n        font-weight: 600;\r\n        font-size: 18px;\r\n        line-height: 24px;\r\n    }\r\n\r\n    .msft-content-description {\r\n        font-family: \"Segoe UI\";\r\n        font-style: normal;\r\n        font-weight: 400;\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n    }\r\n\r\n    .msft-button-primary {\r\n        box-sizing: border-box;\r\n        display: inline-block;\r\n        width: fit-content;\r\n        padding: 0 16;\r\n        height: 24px;\r\n        border-radius: 2px;\r\n        background-color: #0078d4;\r\n        color: #fff;\r\n        text-decoration: none;\r\n        text-align: center;\r\n        font-family: \"Segoe UI\";\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n        cursor: pointer;\r\n        border: none;\r\n        margin-right: 10px;\r\n    }\r\n\r\n    .msft-button-secondary {\r\n        color: #0078d4;\r\n        cursor: pointer;\r\n        background-color: transparent;\r\n        font-family: \"Segoe UI\";\r\n        font-size: 13px;\r\n        line-height: 21px;\r\n    }\r\n\r\n    .msft-model-buttonactions {\r\n        margin-top: 24px;\r\n    }\r\n</style>\r\n<div class=\"msft-cookie-box msft-dialog-box\">\r\n    <div class=\"msft-content\">\r\n        <div class=\"msft-content-header\">{{{header}}}</div>\r\n        <div class=\"msft-content-description\">{{{description}}}</div>\r\n    </div>\r\n    <div class=\"msft-model-buttonactions\">\r\n        <button class=\"msft-button-primary\" id=\"msft-consent-accept\">\r\n            {{{acceptButton}}}\r\n        </button>\r\n        <button class=\"msft-button-primary\" id=\"msft-consent-decline\">\r\n            {{{declineButton}}}\r\n        </button>\r\n        <span class=\"msft-button-secondary\" id=\"msft-consent-settings-open\">{{{customizeButton}}}</span>\r\n    </div>\r\n</div>\r\n<script>\r\n    document.getElementById(\"msft-consent-accept\").onclick = function () {\r\n        MSFTConsent.trigger(\"AcceptAll\");\r\n        MSFTConsent.trigger(\"CloseConsentNotice\");\r\n    };\r\n    document.getElementById(\"msft-consent-settings-open\").onclick = function () {\r\n        MSFTConsent.trigger(\"ShowTemplate\", { \"templateId\": \"Preferences\" });\r\n        MSFTConsent.trigger(\"CloseConsentNotice\");\r\n    };\r\n    document.getElementById(\"msft-consent-decline\").onclick = function () {\r\n        MSFTConsent.trigger(\"RejectAll\");\r\n        MSFTConsent.trigger(\"CloseConsentNotice\");\r\n    };\r\n</script>",
      "location": "ModelLayout/dialog.hbs",
      "fields": [
        "header",
        "description",
        "acceptButton",
        "declineButton",
        "customizeButton"
      ]
    }
